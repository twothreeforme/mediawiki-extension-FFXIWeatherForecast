{
    "name": "FFXIWeatherForecast",
    "version": "0.1.0",
    "author": [
        "Shocks"
    ],
    "url": "none",
    "descriptionmsg": "weatherforecast-desc",
    "license-name": "MIT",
    "type": "other",
    "AutoloadClasses": {
        "HTMLTableHelper": "includes/HTMLTableHelper.php",
        "VanaTime": "includes/VanaTime.php",
        "ExclusionsHelper_Forecast": "includes/ExclusionsHelper_Forecast.php",
        "WeatherForecast_ElementMaps": "includes/WeatherForecast_ElementMaps.php",
        "DBConnection_Forecast": "includes/DBConnection_Forecast.php",
        "ParserHelper_Forecast": "includes/ParserHelper_Forecast.php",
        "DataModel": "includes/DataModel.php",
        "SpecialWeatherForecast": "includes/WeatherForecast.php",
        "SpecialDiggingWeatherForecast": "includes/DiggingWeatherForecast.php"
	},
    "SpecialPages": {
        "WeatherForecast": "SpecialWeatherForecast",
        "DiggingWeatherForecast": "SpecialDiggingWeatherForecast"
    },
    "MessagesDirs": {
        "WeatherForecast": [
			"i18n"
		],
        "DiggingWeatherForecast": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
        "WeatherForecastAlias": "i18n/WeatherForecast.alias.php"
	},
	"ResourceModules": {
		"inputHandler" : {
			"localBasePath": "includes/",
			"remoteExtPath": "WeatherForecast/includes/",
			"packageFiles": [
				"inputHandler.js"
			],
            "dependencies": [
				"oojs-ui-core"
			],
			"styles": [
				"special.css"
			]
		}
	},
    "Hooks": {
        "BeforePageDisplay": "SpecialWeatherForecast::onBeforePageDisplay",
        "ParserFirstCallInit": "WeatherModel::onParserInit"
    },
    "manifest_version": 2,
    "requires": {
        "MediaWiki": ">= 1.39.0",
        "platform": {
            "php": ">= 5.6"
        }

    }
}