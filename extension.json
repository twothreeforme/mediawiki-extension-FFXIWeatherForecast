{
    "name": "FFXIWeatherForecast",
    "version": "0.1.0",
    "author": [
        "Shocks"
    ],
    "url": "none",
    "descriptionmsg": "weatherforecast-desc",
    "license-name": "MIT",
    "type": "other",
    "AutoloadClasses": {
        "SpecialWeatherForecast": "includes/WeatherModel.php",
        "WeatherModel": "includes/WeatherModel.php",
        "ExclusionsHelper": "includes/ExclusionsHelper.php",
        "ParserHelper": "includes/ParserHelper.php",
        "DataModel": "includes/DataModel.php",
        "DBConnection": "includes/DBConnection.php"
	},
    "SpecialPages": {
